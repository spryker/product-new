<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductLabelDictionaryItem">
        <property name="idProductLabel" type="int"/>
    </transfer>

    <transfer name="ProductLabelProductAbstractRelations">
        <property name="idProductLabel" type="int"/>
        <property name="idsProductAbstractToAssign" type="array" singular="idsProductAbstractToAssign"/>
        <property name="idsProductAbstractToDeAssign" type="array" singular="idsProductAbstractToDeAssign"/>
    </transfer>

    <transfer name="ProductAbstract">
        <property name="newTo" type="string"/>
        <property name="newFrom" type="string"/>
    </transfer>

    <transfer name="SearchContext">
        <property name="sourceIdentifier" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="name" type="string"/>
    </transfer>

</transfers>
